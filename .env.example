# PostgreSQL credentials
POSTGRES_USER=your_postgres_username
POSTGRES_PASSWORD=your_postgres_password

# Database names
USERS_DB_NAME=name_of_users_database
HISTORY_DB_NAME=name_of_history_database

# OAuth credentials for Google
GOOGLE_OAUTH_CLIENT_SECRET=your_google_oauth_client_secret
GOOGLE_OAUTH_CLIENT_ID=your_google_oauth_client_id

# Message broker URI (e.g. RabbitMQ)
BROKER_URI=amqp://username:password@hostname:port/vhost

# Database connection URLs (JDBC format)
HISTORY_DB_URL=jdbc:postgresql://hostname:port/history_database_name
USERS_DB_URL=jdbc:postgresql://hostname:port/users_database_name

# JWT secret key (use a strong random string)
JWT_KEY=your_jwt_secret_key

# Redis connection URL
REDIS_URL=redis://hostname:port

# App password for mailing
MAILING_PASSWORD=your_mailing_password